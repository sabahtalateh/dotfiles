{
    "description": "Command+Space to Command +Shift. (for Cmd+Space+F to search in code editors)",
    "manipulators": [
        {
            "from": {
                "key_code": "spacebar",
                "modifiers": { "mandatory": ["command"] }
            },
            "to": [
                {
                    "key_code": "left_shift",
                    "modifiers": ["command"]
                }
            ],
            "type": "basic"
        }
    ]
}